{
  "name": "@greencoast/discord.js-extended",
  "version": "1.2.1",
  "description": "An utility to facilitate the repetitive tasks when creating discord bots. Used by Greencoast Studios.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint ./src ./test",
    "test": "jest --watchAll --silent",
    "test:showlogs": "jest --watchAll",
    "test:once": "jest --silent",
    "test:once-showlogs": "jest",
    "build": "tsc --build",
    "prepublish": "npm run build",
    "docs": "typedoc ./src/index.ts ./src/providers/index.ts ./src/classes/command/default/index.ts --includeVersion --out ./docs --readme ./README.md --plugin typedoc-plugin-extras --favicon ./assets/favicon.ico",
    "docs:watch": "npm run docs -- --watch"
  },
  "engines": {
    "node": ">=16.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greencoast-studios/discord.js-extended.git"
  },
  "keywords": [
    "greencoast",
    "greencoast-studios",
    "discord",
    "discord.js",
    "discord bots"
  ],
  "author": "Greencoast Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greencoast-studios/discord.js-extended/issues"
  },
  "homepage": "https://github.com/greencoast-studios/discord.js-extended#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^27.4.0",
    "@types/level": "^6.0.0",
    "@types/node": "^17.0.8",
    "@types/require-all": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.6",
    "discord.js": "^13.5.0",
    "eslint": "^7.32.0",
    "eslint-config-greencoast": "0.0.2",
    "jest": "^27.4.7",
    "level": "^7.0.1",
    "level-test": "^9.0.0",
    "text-encoding": "^0.7.0",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-extras": "^2.2.3",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "discord.js": "~13",
    "level": "~7"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@greencoast/logger": "^1.1.0",
    "common-tags": "^1.8.2",
    "dayjs": "^1.10.7",
    "humanize-duration": "^3.27.1",
    "require-all": "^3.0.0"
  }
}
