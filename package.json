{
  "name": "@greencoast/discord.js-extended",
  "version": "2.2.3",
  "description": "An utility to facilitate the repetitive tasks when creating discord bots. Used by Greencoast Studios.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "test": "jest --silent",
    "test:showlogs": "jest",
    "test:watch": "jest --watchAll --silent",
    "test:watch-showlogs": "jest --watchAll",
    "build": "tsc --build",
    "prepublish": "npm run build",
    "docs": "typedoc ./src/index.ts ./src/providers/index.ts ./src/classes/command/default/index.ts --includeVersion --out ./docs --readme ./README.md --plugin typedoc-plugin-extras --favicon ./assets/favicon.ico",
    "docs:watch": "npm run docs -- --watch"
  },
  "engines": {
    "node": ">=16.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greencoast-studios/discord.js-extended.git"
  },
  "keywords": [
    "greencoast",
    "greencoast-studios",
    "discord",
    "discord.js",
    "discord bots"
  ],
  "author": "Greencoast Studios",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greencoast-studios/discord.js-extended/issues"
  },
  "homepage": "https://github.com/greencoast-studios/discord.js-extended#readme",
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/jest": "^29.5.3",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^20.4.2",
    "@types/require-all": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "babel-jest": "^29.6.1",
    "discord.js": "^14.11.0",
    "eslint": "^8.45.0",
    "eslint-config-greencoast": "^0.0.4",
    "jest": "^29.6.1",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-extras": "^2.3.3",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "discord.js": "~14"
  },
  "dependencies": {
    "@greencoast/logger": "^1.1.1",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.9",
    "humanize-duration": "^3.29.0",
    "intl-messageformat": "^10.5.0",
    "lodash.clonedeep": "^4.5.0",
    "require-all": "^3.0.0"
  }
}
